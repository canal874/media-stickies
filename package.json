{
  "name": "media-stickies",
  "productName": "Media Stickies",
  "version": "1.0.11",
  "description": "Media Stickies",
  "main": "dist/main.js",
  "scripts": {
    "compile-clean": "rm -r dist && cp -r html dist && cp -r 3rdParty/* dist/ && cp -r assets dist/ && npx eslint --fix --ext .ts .  && tsc",
    "compile": "npx eslint --fix --ext .ts . && tsc",
    "compile-quick": "tsc",
    "start": "tsc && electron-forge start",
    "package": "npx eslint --fix --ext .ts . && electron-forge package",
    "make": "rm -r dist && cp -r html dist && cp -r 3rdParty/* dist/ && cp -r assets dist/ && npx eslint --fix --ext .ts . && tsc && electron-forge make",
    "publish": "npx eslint --fix --ext .ts . && electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": "Hidekazu Kubota",
  "license": "MPL-2.0",
  "config": {
    "forge": {
      "packagerConfig": {
        "appCopyright": "Copyright(C) 2020 Hidekazu Kubota",
        "asar": true,
        "icon": "assets/media_stickies_grad_icon",
        "win32metadata": {
          "ProductName": "Media Stickies",
          "FileDescription": "Media Stickies - A desktop gadget platform"
        }
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "media_stickies"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.50",
    "@electron-forge/maker-deb": "^6.0.0-beta.50",
    "@electron-forge/maker-rpm": "^6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.50",
    "@electron-forge/maker-zip": "^6.0.0-beta.50",
    "@types/node": "^12.12.37",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "electron": "^8.2.3",
    "eslint": "^6.8.0",
    "eslint-config-standardize": "^0.6.1",
    "eslint-plugin-prettierx": "^0.12.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/ckeditor": "^4.9.9",
    "@types/pouchdb": "^6.4.0",
    "@types/uniqid": "^4.1.3",
    "dayjs": "^1.8.28",
    "dayjs-plugin-utc": "^0.1.2",
    "electron-context-menu": "^1.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "log4js": "^6.2.1",
    "pouchdb": "^7.1.1",
    "typed-intl": "^1.0.3",
    "uniqid": "^5.2.0"
  }
}
